<h1>Books</h1>

<% @books.each do |book| %>

  <h4>Title</h4>
  <span><%= book.title %></span>

  <h4>Body</h4>
  <span><%= book.body %><//span>

  <%= link_to "Show", book_path(book.id) %>
  <%= link_to "Edit", edit_book_path(book.id) %>
  <%= link_to "Destroy", books_path(book.id) %>
<% end %>



